      </Label>
      <Label layoutX="102.0" layoutY="170.0" text="User ID">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="txtuid" layoutX="225.0" layoutY="170.0" />
      <PasswordField fx:id="txtpassword" layoutX="225.0" layoutY="235.0" />
      <Label layoutX="94.0" layoutY="235.0" text="Password">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="loginbtn" layoutX="255.0" layoutY="325.0" mnemonicParsing="false" onAction="#check" text="Login" />
